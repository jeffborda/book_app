<!DOCTYPE html>

<html>
    <head></head>
    <body>
        <h1>Book Google Result</h1>
        <p><a href="/">Home</a></p>
        <ul>
            <% bookList.forEach( book => { %>
                <li><%=book.title %></li>
                <li><%=book.author %></li>
                <li><img src="<%=book.img_url %>" alt="book image"></li>
                <li><%=book.description %></li>
            <% }) %>
        </ul>
    </body>
</html>